    "@types/morgan": "^1.7.35",
    "@types/node": "^10.11.4",
    "errorhandler": "^1.5.0",
    "husky": "^1.1.1",
    "jest": "^23.6.0",
    "jest-junit": "^5.2.0",
    "nodemon": "^1.18.4",
    "prettier": "1.14.3",
    "pretty-quick": "^1.7.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "ts-jest": "^23.10.4",
